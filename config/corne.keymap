    &caps_word {
        continue-list = <UNDERSCORE MINUS LALT N0 KP_N0 N1 N2 N3 N4 N5 N6 N7 N8 N9 NON_US_HASH LEFT_BRACKET>;
    };

/ {
 
    combos {
        compatible = "zmk,combos";

        combo_esc {
            timeout-ms = <50>;
            bindings = <&kp ESC>;
            key-positions = <0 1>;
        };

        combo_gamelayer {
            timeout-ms = <50>;
            key-positions = <37 40>;
            bindings = <&tog 5>;
        };

        combo_settinglayer {
            timeout-ms = <50>;
            key-positions = <>;
            bindings = <&tog 3>;
            layers = <3>;
        };



        combo_shiftalt {
            timeout-ms = <50>;
            key-positions = <12 13>;
            bindings = <&caps_word>;
        };

        combo_del {
            timeout-ms = <50>;
            key-positions = <10 11>;
            bindings = <&kp DEL>;
        };

        combo_gui {
            timeout-ms = <50>;
            key-positions = <22 23>;
            bindings = <&kp LGUI>;
        };
    };

