 // HOLD TAP
        a_ht: a_hold_tap {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            tapping-term-ms = <220>;
            require-prior-idle-ms = <200>;
            flavor = "tap-preferred";
            bindings = <&mod_a>, <&kp>;
        };

        e_ht: e_hold_tap {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            tapping-term-ms = <220>;
