    keymap {
        compatible = "zmk,keymap";

        base_layer {
            // -----------------------------------------------------------------------------------------
            // |  TAB |  Q  |  W  |  E  |  R  |  T  |   |  Y  |  U   |  I  |  O  |  P  | BKSP |
            // | CTRL |  A  |  S  |  D  |  F  |  G  |   |  H  |  J   |  K  |  L  |  ;  |  '   |
            // | SHFT |  Z  |  X  |  C  |  V  |  B  |   |  N  |  M   |  ,  |  .  |  /  | ESC  |
            //                    | GUI | LWR | SPC |   | ENT | RSE  | ALT |
            //   3201234678901235678
            
        
            display-name = "CSALLAP";
            bindings = <
&kp TAB    &kp Q      &kp W     &e_ht 0 E  &kp R      &kp T        &kp Y    &td_u     &i_ht 0 I  &td_o    &kp P     &kp BSPC
&td_shiftalt  &a_ht 0 A  &mod_left &mod_down  &mod_right &kp G        &kp H    &kp J     &kp K      &kp L    &td_sdqt  &td_shiftalt
&td_cshift &kp Z      &kp X     &td_ctrl_c &td_ctrl_v &kp B        &kp N    &kp M     &kp COMMA  &kp DOT  &kp FSLH  &td_cshift
                                &td_ag     &tp_lyr1   &mt LA(TAB) SPACE   &mt LG(LC(LALT)) SPACE &tp_lyr2  &kp RALT
            >;
        };

        num_layer {
            display-name = "UTYULU-PUTYULU";
            bindings = <
&trans  &kp KP_N0 &td_n1f1  &td_n2f2  &td_n3f3  &td_n4f4           &kp C_VOLUME_UP   &none          &mt PG_UP UP    &none          &kp PRINTSCREEN  &trans
&trans  &kp N5    &td_n6f5  &td_n7f6  &td_n8f7  &td_n9f8           &kp C_VOLUME_DOWN &mt HOME LEFT  &mt PG_DN DOWN  &mt END RIGHT  &none            &trans
&trans  &none     &kp F9    &kp F10   &kp F11   &kp F12            &kp C_MUTE        &none          &none           &none          &none            &trans
                            &trans    &trans    &trans             &trans            &trans         &trans
            >;
        };
        mirror_num_layer {
            display-name = "PUTYULU-UTYULU";
            bindings = <
&trans  &kp PRINTSCREEN  &none         &mt PG_UP UP    &none          &kp C_VOLUME_UP            &td_n4f4 &td_n3f3  &td_n2f2  &td_n1f1  &kp KP_N0 &trans
&trans  &none            &mt HOME LEFT &mt PG_DN DOWN  &mt END RIGHT  &kp C_VOLUME_DOWN          &td_n9f8 &td_n8f7  &td_n7f6  &td_n6f5  &kp N5    &trans
&trans  &none            &none         &none           &none          &kp C_MUTE                 &kp F12  &kp F11   &kp F10   &kp F9    &none     &trans
                                       &trans          &trans         &trans                     &trans   &trans    &trans
            >;
        };

        bracket_layer {
            // -----------------------------------------------------------------------------------------
            // |  TAB |   ^  |   *  | -/+ |  (  |  )  |        |  &  |    !  |   :  |  #  |     | DEl |
            // | CTRL |     |      |  =  |  [  |  ]  |        |  <>  |  ?  |  _   |   $  |    |     |
            // | SHFT |  %  |  /  |  \  |  {  |  }  |        |  |  |      |     |   @    |     |     |
            //                    | GUI |     | SPC |        | ENT |     | ALT |
            //


            display-name = "KIS CICA";
            bindings = <
&trans  &alt_CARET  &alt_ASTERIX  &td_pm      &alt_LPAR   &alt_RPAR     &alt_AMPS  &alt_EXCL &alt_TILDE  &alt_HASH  &none    &trans
&trans  &none       &none         &alt_EQUAL  &alt_LSQBR  &atl_RSQBR    &td_ltgt   &alt_QESTN  &alt_UNDER &alt_DLLR  &alt_AT  &trans
&trans  &alt_PRCNT  &alt_FSLH     &alt_BSLH   &alt_LCRLY  &alt_RCRLY    &alt_PIPE  &none     &none       &none      &none    &trans
                                  &trans      &trans      &trans        &trans     &trans    &trans
            >;
